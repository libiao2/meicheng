<block wx:if="{{$taroCompReady}}">
    <view class="container">
        <at-tabs compid="{{$compid__360}}">
            <at-tabs-pane wx:for="{{loopArray136}}" wx:for-item="item" wx:for-index="index" compid="{{item.$compid__359}}">
                <block>
                    <block wx:if="{{orderList.length > 0}}">
                        <view class="orderBox" wx:for="{{item.$anonymousCallee__22}}" wx:for-item="item2" wx:for-index="index2"><text class="name">{{item2.$original.name}}</text>
                            <view class="orderInfo" bindtap="anonymousFunc0" wx:for="{{item2.$anonymousCallee__21}}" wx:for-item="item3" wx:for-index="index3" data-e-tap-so="this" data-e-tap-a-a="{{item3._$indexKey}}">
                                <view class="imgBox">
                                    <image class="img" src="{{item3.$original.pic}}"></image>
                                    <view class="type"><text>{{item3.$loopState__temp2}}</text>
                                    </view>
                                </view>
                                <view class="countInfo"><text>{{item3.$original.productItemName}}</text><text style="color: red">${{item3.$original.price}}</text><text>x {{item3.$original.quantity}}</text>
                                </view>
                            </view>
                            <view class="bottomBtnBox">
                                <view class="allPrice">总价: $<text style="color: red">{{item2.$original.orderFeeCNY}}</text>
                                </view>
                                <view class="box">
                                    <view class="oneBtn" bindtap="anonymousFunc1" data-e-tap-so="this" data-e-tap-a-a="{{item2._$indexKey2}}">更多</view>
                                    <block wx:if="{{item2.$original.orderStatus == 10}}">
                                        <view class="oneBtn nowPay" bindtap="anonymousFunc2" data-e-tap-so="this" data-e-tap-a-a="{{item2._$indexKey3}}">立即支付</view>
                                    </block>
                                    <block wx:if="{{item2.$original.orderStatus == 40}}">
                                        <view class="oneBtn nowPay" bindtap="anonymousFunc3" data-e-tap-so="this" data-e-tap-a-a="{{item2._$indexKey4}}">退款进度</view>
                                    </block>
                                    <block wx:if="{{item2.$original.orderStatus == 50}}">
                                        <view class="oneBtn" bindtap="anonymousFunc4" data-e-tap-so="this" data-e-tap-a-a="{{item2._$indexKey5}}">退款进度</view>
                                    </block>
                                </view>
                            </view>
                        </view>
                    </block>
                    <block wx:else>
                        <view class="noData">暂无更多数据~</view>
                    </block>
                </block>
            </at-tabs-pane>
        </at-tabs>
        <at-action-sheet compid="{{$compid__361}}">
            <at-action-sheet-item>
                <view class="payTitle"><text>请选择支付方式</text>
                </view>
                <view class="payBox">
                    <view class="oneBox" bindtap="anonymousFunc6">
                        <view class="payImgBox">
                            <image class="payImg" src="{{anonymousState__temp3}}"></image><text>微信支付(人民币)</text>
                        </view>
                        <at-icon compid="{{$compid__362}}"></at-icon>
                    </view>
                    <view class="oneBox" bindtap="anonymousFunc7">
                        <view class="payImgBox">
                            <image class="payImg" src="{{anonymousState__temp4}}"></image><text>信用卡支付(外币)</text>
                        </view>
                        <at-icon compid="{{$compid__363}}"></at-icon>
                    </view>
                    <button open-type="share" class="oneBox">
                        <view class="payImgBox">
                            <image class="payImg" src="{{anonymousState__temp5}}"></image><text>好友代付(外币)</text>
                        </view>
                        <at-icon compid="{{$compid__364}}"></at-icon>
                    </button>
                </view>
            </at-action-sheet-item>
        </at-action-sheet>
        <at-modal compid="{{$compid__365}}">
            <at-modal-content>
                <view class="titleBox"><text>选择信用卡支付</text><text class="delet" bindtap="anonymousFunc8">X</text>
                </view>
                <view class="toastBox">
                    <block wx:if="{{creditCardList.length > 0}}">
                        <view bindtap="anonymousFunc9" class="{{'oneCard ' + (item.$original.defaultSource && 'isChooseCard')}}" wx:for="{{loopArray137}}" wx:for-item="item" wx:for-index="index" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey6}}"><text>卡号后四位</text><text style="color: #333">({{item.$original.last4}})</text>
                        </view>
                    </block>
                    <view class="addPayCard" bindtap="anonymousFunc10"><text>+添加新卡支付</text>
                    </view>
                </view>
                <view class="surePayBtn">
                    <view class="payBtn" bindtap="anonymousFunc11"><text>立即支付</text>
                    </view>
                </view>
            </at-modal-content>
        </at-modal>
    </view>
</block>