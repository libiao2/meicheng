<block wx:if="{{$taroCompReady}}">
    <view class="container">
        <view class="titleBox"><text>基本信息</text>
        </view>
        <view class="itemBox"><text class="leftPart">持卡人姓名:</text>
            <input type="text" class="shopNameInput" placeholder="请输入持卡人姓名" value="{{name}}" bindchange="nameChange" data-e-change-so="this" />
        </view>
        <view class="itemBox"><text class="leftPart">卡号:</text>
            <input type="text" class="shopNameInput" placeholder="请输入卡号" value="{{number}}" bindchange="numberChange" data-e-change-so="this" />
        </view>
        <view class="itemBox"><text class="leftPart">有效期限:</text>
            <picker mode="date" fields="month" bindchange="onEndDateChange">
                <view class="picker"><text>{{endTime}}</text>
                    <at-icon compid="{{$compid__358}}"></at-icon>
                </view>
            </picker>
        </view>
        <view class="itemBox"><text class="leftPart">CVC:</text>
            <input type="text" class="shopNameInput" placeholder="请输入CVC" value="{{cvc}}" bindchange="cvcChange" data-e-change-so="this" />
        </view>
        <view class="titleBox"><text>账单邮寄地址</text>
        </view>
        <view class="itemBox"><text class="leftPart">持卡人姓名:</text>
            <input type="text" class="shopNameInput" placeholder="请输入持卡人姓名" value="{{shippingName}}" bindchange="shippingNameChange" data-e-change-so="this" />
        </view>
        <view class="itemBox"><text class="leftPart">国家:</text>
            <input type="text" class="smallInput" placeholder="请填写国家" value="{{country}}" bindchange="countryChange" data-e-change-so="this" /><text class="zhou">州</text>
            <input type="text" class="smallInput" placeholder="请填写州" value="{{state}}" bindchange="stateChange" data-e-change-so="this" />
        </view>
        <view class="itemBox"><text class="leftPart">城市:</text>
            <input type="text" class="smallInput" placeholder="请填写城市" value="{{city}}" bindchange="cityChange" data-e-change-so="this" /><text class="zhou">邮编</text>
            <input type="text" class="smallInput" placeholder="请填写邮编" value="{{postalCode}}" bindchange="postalCodeChange" data-e-change-so="this" />
        </view>
        <view class="itemBox"><text class="leftPart">街道1:</text>
            <input type="text" class="shopNameInput" placeholder="请填写详细的街道地址" value="{{line1}}" bindchange="line1Change" data-e-change-so="this" />
        </view>
        <view class="itemBox"><text class="leftPart">街道2:</text>
            <input type="text" class="shopNameInput" placeholder="请填写详细的门牌号码" value="{{line2}}" bindchange="line2Change" data-e-change-so="this" />
        </view>
        <view class="btn">
            <view class="btnBox" bindtap="anonymousFunc0"><text>添加信用卡</text>
            </view>
        </view>
    </view>
</block>