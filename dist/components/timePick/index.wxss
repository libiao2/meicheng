.datetime-picker-wrap .selector-wrap {
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  background: #FFFFFF;
  color: #9BA0AA;
  padding: 0 20rpx;
}
.datetime-picker-wrap .selector-wrap .select-item {
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
  font-size: 30rpx;
  padding: 12rpx 0;
}
.datetime-picker-wrap .wrapper .model-box-bg {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10000;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.3;
}
.datetime-picker-wrap .wrapper .model-box {
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 999999;
  width: 100%;
  background: #fff;
}
.datetime-picker-wrap .wrapper .model-picker {
  position: relative;
}
.datetime-picker-wrap .wrapper .model-picker .button-model {
  height: 80rpx;
  width: 100%;
  background: #fff;
  position: relative;
  border-bottom: 1rpx solid #d9d9d9;
}
.datetime-picker-wrap .wrapper .model-picker .button-model .btn-txt {
  color: #007aff;
  position: absolute;
  background: transparent;
  border: none;
  line-height: 80rpx;
}
.datetime-picker-wrap .wrapper .model-picker .button-model .btn-txt:first-child {
  left: 32rpx;
}
.datetime-picker-wrap .wrapper .model-picker .button-model .btn-txt:last-child {
  right: 32rpx;
}
.datetime-picker-wrap .wrapper .model-picker .pick-view {
  width: 100%;
  height: 600rpx;
}
.datetime-picker-wrap .wrapper .model-picker .pick-view .picker-view-column {
  text-align: center;
}
.datetime-picker-wrap .wrapper .model-picker .pick-view .picker-view-column .pick-view-column-item {
  line-height: 50PX;
}

